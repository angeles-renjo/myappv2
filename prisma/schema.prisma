datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"

}


model Trade {
 id          String   @id @default(auto()) @map("_id") @db.ObjectId
 pair        String
 rule        String
 risk        String
 learnings   String?
 tradeType   String
 profitLoss Float?
 date        String
 link        String?
 userId      String   @db.ObjectId
 userEmail      String
 @@unique([userEmail])
}